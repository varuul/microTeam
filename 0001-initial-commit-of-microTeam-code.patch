+			this.status = "open";
+		});
+	}	
+	this.updateLabel();
+}
+
+FrameSlide.prototype.close = function() {
+	if (this.type == "top") {
+		$(this.DOMelement).animate({
+			opacity: this.inactive_opacity,
+			top: this.inactive_position()
+		}, this.animationSpeed, 
+		 function() {
+			this.status = "closed";
+		});
+	}	
+	this.updateLabel();
